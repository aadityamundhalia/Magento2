<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="cron_newproducts"> <!-- Doesn't need to be unique -->
        <job name="cron_newproduct" instance="Project\Newproducts\Cron\Newproduct" method="execute">
            <schedule>0 6 * * 6</schedule>
        </job>
    </group>
</config>
